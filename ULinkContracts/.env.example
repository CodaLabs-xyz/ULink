# RPC URLs
ALCHEMY_API_KEY=your_alchemy_api_key_here
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org
BASE_MAINNET_RPC_URL=https://mainnet.base.org

# Private key for deployment (use test key only!)
DEPLOYER_PRIVATE_KEY=your_deployer_private_key_here
# Note: ADMIN_PRIVATE_KEY not needed for deployment - admin can be same as deployer

# Etherscan API keys for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here
BASESCAN_API_KEY=your_basescan_api_key_here

# Contract addresses (filled after deployment)
PROJECT_REGISTRY_ADDRESS=
MEMBERSHIP_NFT_ADDRESS=
ANALYTICS_TRACKER_ADDRESS=

# Deployment settings (optional - will derive from DEPLOYER_PRIVATE_KEY if not set)
DEPLOYER_ADDRESS=0x0000000000000000000000000000000000000000
ADMIN_ADDRESS=0x0000000000000000000000000000000000000000  # Defaults to deployer if not set
TREASURY_ADDRESS=0x0000000000000000000000000000000000000000  # Defaults to admin if not set